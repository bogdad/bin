rm top.log
rm top_cocained.log
rm top_cocaine_slave.log
rm perftest.log
rm gc.log
rm jstat.log
src/test/resources/run_cocaine.sh &
sleep 2s
echo runnint ant perftest
src/test/resources/run_client.sh &>perftest.log &
sleep 2s
perfpid=`greppid MainSingleThreaded`
echo perfpid =$perfpid
echo running jstat
jstat -gc -h10 $perfpid 20000 999999 > jstat.log &
echo running top ant
top -b -p $perfpid > top.log &
echo top cocained
echo cocained_pid = `greppid cocained`
echo cocaine salve pid = `greppid cocaine-slave`
top -b -p `greppid cocained` > top_cocained.log &
echo top cocaine-slave
top -b -p `greppid cocaine-slave` > top_cocaine_slave.log &
echo all started
rm dealer.log
